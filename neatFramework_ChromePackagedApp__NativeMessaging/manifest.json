{
  "name": "neatFramework ChPaApp: RickShaw EEG",
  "short_name" : "nF App: Rs_EEG",
  "description":"neatFramework-based Chrome packaged app displaying real-time EEG data",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "offline_enabled": true,
  "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "64": "icons/icon-64.png",
        "128": "icons/icon-128.png",
        "256": "icons/icon-256.png"
  },

  "app": { 
      "background": {
                 "scripts": [ "js/background.js" ]
      }
  },
    
  "permissions": [ "fullscreen", "alwaysOnTopWindows", "serial", "http://localhost:*/*", "file:///*", "http://188.226.162.97:*/*", "nativeMessaging" ],
  "sockets": {
    "udp": { "send": "*:*",
             "bind": "*:*"
    },
    "tcp": { "connect": "*:*"
    },
    "tcpServer": { "listen": "*:*"
    }
  }

}
